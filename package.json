  "description": "Species recognition and recall",
  "main": "index.js",
  "scripts": {
    "test": "jest-webpack",
    "start:dev": "webpack-dev-server --open-page 'dist/index.html' --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js & jest --watchAll"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "html"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "ui/screens/.*/.*.html": "<rootDir>/src/empty.js",
      "ui/progress/.*.html": "<rootDir>/src/empty.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danhartley/snapdragon-redux.git"
  },
  "keywords": [
    "species",
    "recognition",
    "identification",
    "learn",
    "recall",
    "eol"
  ],
  "author": "Daniel Hartley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danhartley/snapdragon-redux/issues"
  },
  "homepage": "https://github.com/danhartley/snapdragon-redux#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
