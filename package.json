{
  "name": "learn-the-planet",
  "version": "1.0.0",
  "description": "Species recognition and recall",
  "main": "index.js",
  "scripts": {
    "test": "jest-webpack",
    "start:dev": "webpack-dev-server --config ./webpack.dev.js --open-page 'dist/index.html'  & jest --watchAll",
    "start:notestdev": "webpack-dev-server --config ./webpack.dev.js --open-page 'dist/index.html'",
    "start:device": "webpack-dev-server --config ./webpack.device.js --open-page 'dist/index.html'",
    "start:prod": "webpack-dev-server --config ./webpack.prod.js --open-page 'dist/index.html'",
    "start:admin": "webpack-dev-server --config ./webpack.admin.js --open-page 'dist/collection-builder.html'",
    "start:checklist": "webpack-dev-server --config ./webpack.checklist.js --open-page 'dist/checklist.html'",
    "start:quiz": "webpack-dev-server --config ./webpack.quiz.js --open-page 'dist/quiz.html' & jest ./src/quiz --watchAll",
    "start:flashcards": "webpack-dev-server --config ./webpack.flashcards.js --open-page 'dist/flashcards.html'",
    "dev": "webpack --config ./webpack.dev.js && workbox generateSW workbox-config.js",
    "device": "webpack --config ./webpack.device.js",
    "dev:analyse": "webpack --config ./webpack.dev.js -- --env.addons=bundleanalyzer",
    "prod": "webpack --config ./webpack.prod.js && workbox generateSW workbox-config.js",
    "admin": "webpack --config ./webpack.admin.js",
    "checklist": "webpack --config ./webpack.checklist.js",
    "quiz": "webpack --config ./webpack.quiz.js",
    "flashcards": "webpack --config ./webpack.flashcards.js",
    "watch": "webpack --watch --config ./webpack.dev.js & jest --watchAll"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "js",
      "html"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "ui/screens/.*/.*.html": "<rootDir>/src/empty.js",
      "ui/progress/.*.html": "<rootDir>/src/empty.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danhartley/snapdragon-redux.git"
  },
  "keywords": [
    "species",
    "recognition",
    "identification",
    "learn",
    "recall",
    "eol"
  ],
