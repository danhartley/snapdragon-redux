    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.1.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "i": "^0.3.6",
    "immutable": "^3.8.2",
    "jest": "^22.4.2",
    "jest-webpack": "^0.4.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "ramda": "^0.25.0",
    "redux": "^3.7.2",
